    <!-- Usuarios -->
    <servlet>
        <description>Listado de usuarios</description>
        <servlet-name>listaUsuarios</servlet-name>
        <servlet-class>usuarios.lista</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>listaUsuarios</servlet-name>
        <url-pattern>/usuarios/lista</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>Inserta o actualiza un usuario</description>
        <servlet-name>guardarUsuario</servlet-name>
        <servlet-class>usuarios.guardar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>guardarUsuario</servlet-name>
        <url-pattern>/usuarios/guardar</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <description>Elimina un usuario de la base de datos</description>
        <servlet-name>eliminarUsuario</servlet-name>
        <servlet-class>usuarios.eliminar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>eliminarUsuario</servlet-name>
        <url-pattern>/usuarios/eliminar</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <description>Listado de grupos de un usuario</description>
        <servlet-name>gruposUsuario</servlet-name>
        <servlet-class>usuarios.gruposUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gruposUsuario</servlet-name>
        <url-pattern>/usuarios/gruposUsuario</url-pattern>
    </servlet-mapping>
    
    <servlet>
